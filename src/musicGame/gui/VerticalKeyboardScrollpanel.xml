<?xml version="1.0" encoding="UTF-8"?>
<nifty-controls
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../../../config/nifty-1.3.xsd">
	
	<controlDefinition name="verticalKeyboardScrollpanel" controller="musicGame.gui.VerticalKeyboardScrollpanel"
		childRootId="#childRoot">
		<panel id="#border" style="verticalKeyboardScrollpanel-border">
			<panel id="#background" style="verticalKeyboardScrollpanel-background">
				<panel id="#childRoot" style="verticalKeyboardScrollpanel-childRoot"/>
				<panel id="#scrollArea" style="verticalKeyboardScrollpanel-scrollArea">
					<panel id="scroller" style="verticalKeyboardScrollpanel-scroller"/>
				</panel>
			</panel>
		</panel>
	</controlDefinition>
</nifty-controls>